{
  "rules": {
    ".read": "auth != null",
    ".write": "auth != null",
    "logs": {
      "$uid": {
        ".indexOn": [".value", "checkInDate", "checkOutTime", "projectName"]
      }
    },
    "projects": {
      "$uid": {
        ".indexOn": [".value", "projectName", "createdAt", "reports"]
      }
    }
  }
}
